let dataset = [];

async function loadData() {
  const response = await fetch('biomass_sensor_database.csv');
  const csvText = await response.text();

  Papa.parse(csvText, {
    header: true,
    skipEmptyLines: true,
    complete: function(results) {
      dataset = results.data;
      populateFilters();
      filterData();
    }
  });
}
